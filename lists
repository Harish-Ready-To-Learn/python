import sys
from array import *
def arr():
     r=array('i',[1,2,3,4,5,6])
     for j in range(0,r.__len__(),2):
            print(r[j])
arr()
print(sys.version_info)
def lis():
    h=[23,36,95,14,12,26]
    h.remove(95)
    h.__delitem__(0)
    h.append(95)
    h.pop(3)
    del h[1:3]
    print(h)
lis()


